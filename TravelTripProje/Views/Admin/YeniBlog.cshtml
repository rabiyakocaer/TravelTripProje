@model TravelTripProje.Models.Siniflar.Blog
@{
    ViewBag.Title = "YeniBlog";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>YeniBlog</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.Başlık)
    @Html.TextBoxFor(x => x.Başlık, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Tarih)
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.BlogImage)
    @Html.TextBoxFor(x => x.BlogImage, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Açıklama)
    @Html.TextBoxFor(x => x.Açıklama, new { @class = "form-control" })
    <br />
    <button class="btn btn-danger">Blog Ekle</button>
</form>
