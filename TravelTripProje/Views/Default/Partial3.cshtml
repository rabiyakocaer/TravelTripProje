@using TravelTripProje.Models.Siniflar
@model List<Blog> 
<div class="padding">
    <h4>En Popüler Bloglar</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var x in Model)
        {


        <li class="list-bg"><span>@{ i++; }@i.</span><a href="/Blog/BlogDetay/@x.ID">@x.Başlık</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">Tüm blogları gör</a>
</div>